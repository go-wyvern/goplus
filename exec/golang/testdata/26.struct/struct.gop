type Person struct {
	Name    string
	Age     int
	Friends []string
}

func (p *Person) SetName(name string) {
	p.Name = name
	println(p.Name)
}

func (p Person) SetAge(age int) {
	p.Age = age
	println(p.Age)
}

p := &Person{
	Name: "bar",
	Age:  30,
}

p.Name, p.Age = "bar2", 31

p.SetName("foo")
p.SetAge(31)

println(p.Name)
println(p.Age)
